{
	"name": "official",
	"version": "1.0.0",
	"description": "Official stages of HackforPlay",
	"main": "entry.js",
	"scripts": {
		"start": "webpack-dev-server",
		"build": "NODE_ENV=production webpack -p",
		"test": "ava",
		"postinstall": "npm run build",
		"prettier": "prettier --config ./.prettierrc --write '**/*.js'"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hackforplay/official.git"
	},
	"keywords": [
		"HackforPlay"
	],
	"author": "teramotodaiki",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/hackforplay/official/issues"
	},
	"homepage": "https://github.com/hackforplay/official#readme",
	"devDependencies": {
		"@babel/preset-env": "^7.0.0-beta.52",
		"@babel/register": "^7.0.0-beta.52",
		"ava": "^1.0.0-beta.6",
		"canvas-prebuilt": "^1.6.5-prerelease.1",
		"es6-promisify": "^5.0.0",
		"handlebars": "^4.0.11",
		"handlebars-loader": "^1.7.0",
		"html-webpack-plugin": "^3.2.0",
		"offline-plugin": "^5.0.5",
		"open-browser-webpack-plugin": "0.0.5",
		"prettier": "^1.12.1",
		"unorm": "^1.4.1",
		"webpack": "^4.20.2",
		"webpack-cli": "^3.1.2",
		"webpack-dev-server": "^3.1.9",
		"window": "^4.2.5"
	},
	"dependencies": {
		"compression": "^1.7.2",
		"connect": "^3.6.6",
		"cors": "^2.8.4",
		"feeles-ide": "^1.7.0",
		"morgan": "^1.9.0",
		"serve-static": "^1.13.2"
	},
	"ava": {
		"files": [
			"test/**/*.spec.js",
			"!test/helpers/**/*.js"
		],
		"cache": true,
		"concurrency": 5,
		"failFast": true,
		"failWithoutAssertions": false,
		"tap": true,
		"compileEnhancements": false,
		"require": [
			"@babel/register"
		],
		"babel": {
			"testOptions": {
				"presets": [
					[
						"@babel/preset-env",
						{
							"targets": {
								"node": "current"
							}
						}
					]
				]
			}
		}
	}
}
